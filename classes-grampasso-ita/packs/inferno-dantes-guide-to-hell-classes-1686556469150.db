{"name":"Unguento Per Lame","type":"consumable","img":"modules/classes-grampasso-ita/assets/icons/consumables/potions/bottle-conical-corked-yellow.webp","effects":[{"label":"Unguento","icon":"icons/consumables/potions/bottle-conical-corked-yellow.webp","changes":[{"key":"macro.itemMacro","mode":0,"value":"","priority":20}],"transfer":false,"_id":"3nP196nTaWN4Oys6","disabled":false,"duration":{"startTime":null,"seconds":3600,"combat":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"origin":null,"tint":null,"flags":{"dae":{"selfTarget":false,"selfTargetAlways":false,"stackable":"none","durationExpression":"","macroRepeat":"none","specialDuration":[]},"core":{"statusId":""},"ActiveAuras":{"isAura":false,"aura":"None","radius":null,"alignment":"","type":"","ignoreSelf":false,"height":false,"hidden":false,"displayTemp":false,"hostile":false,"onlyOnce":false}}}],"flags":{"babele":{"translated":true,"hasTranslation":true,"originalName":"Potion of Greater Healing"},"core":{"sourceId":"Compendium.dnd5e.items.L887NdWEP5NqHCrQ"},"midi-qol":{"effectActivation":false},"magicitems":{"enabled":false,"equipped":false,"attuned":false,"charges":"0","chargeType":"c1","destroy":false,"destroyFlavorText":"ha esaurito le cariche: si sbriciola in cenere e viene distrutto.","rechargeable":false,"recharge":"0","rechargeType":"t1","rechargeUnit":"r1","sorting":"l"},"betterCurses":{"isCurse":false,"curseName":"","formula":"","mwak":false,"rwak":false,"msak":false,"rsak":false},"midiProperties":{"nodam":false,"fulldam":false,"halfdam":false,"rollOther":false,"critOther":false,"magicdam":false,"magiceffect":false,"concentration":false,"toggleEffect":false,"autoFailFriendly":false,"offHandWeapon":false,"ignoreTotalCover":false},"cf":{"id":"temp_oeyiemn1rgd"},"itemacro":{"macro":{"name":"Unguento Per Lame","type":"script","scope":"global","command":"const lastArg = args[args.length - 1];\nconst castItemName = \"Unguento\";\nconst tokenOrActor = await fromUuid(lastArg.actorUuid);\nconst targetActor = tokenOrActor.actor ? tokenOrActor.actor : tokenOrActor;\n\n\n/**\n * Create Arma Unguento item in inventory\n */\nif (args[0] === \"on\") {\n  const tokenFromUuid  = await fromUuid(lastArg.tokenUuid);\n  const targetToken = tokenFromUuid.data || token;\n  const DAEItem = lastArg.efData.flags.dae.itemData;\n  const damage = Math.floor(Math.floor(args[1]/ 2));\n\n  const castItem = targetActor.items.find((i) => i.name === castItemName && i.type === \"weapon\");\n  if (!castItem) {\n    const weaponData = {\n      name: castItemName,\n      type: \"weapon\",\n      data: {\n        quantity: 1,\n        activation: { type: \"special\", cost: 1, condition: \"\", },\n        target: { value: 1, type: \"creature\", },\n        // we choose 20ft here in case the midi-check range is on\n        range: { value: 10, long: null, units: \"ft\", },\n        ability: DAEItem.system.ability,\n        attackBonus: DAEItem.system.attackBonus,\n        actionType: \"utility\",\n        chatFlavor: \"\",\n        critical: null,\n        damage: { parts: [[`1d4`, \"force\"]], versatile: \"\" },\n        weaponType: \"simpleM\",\n        proficient: true,\n        equipped: true,\n      },\n      flags: { Unguento: targetActor.id },\n      img: DAEItem.img,\n    };\n\n    await targetActor.createEmbeddedDocuments(\"Item\", [weaponData]);\n    ui.notifications.notify(\"Danno Extra Unguento Creato Nel Tuo Inventario\");\n  }\n}\n\n// Delete Shadow Blade\nif (args[0] === \"off\") {\n  let swords = targetActor.items.filter((i) => i.flags?.Unguento=== targetActor.id);\n  if (swords.length > 0) await targetActor.deleteEmbeddedDocuments(\"Item\", swords.map((s) => s.id));\n  const templates = canvas.templates.placeables.filter((i) => i.data.flags?.Unguento?.ActorId === targetActor.id);\n  if (templates.length > 0) await canvas.scene.deleteEmbeddedDocuments(\"MeasuredTemplate\", templates.map((t) => t.id));\n}","author":"VBXYIar4ySHsR5aH","_id":null,"img":"icons/svg/dic